{
    "preset": "laravel",
    "rules": {
        "array_syntax": {
            "syntax": "short"
        },
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "no_unused_imports": true,
        "not_operator_with_successor_space": true,
        "phpdoc_scalar": true,
        "phpdoc_separation": true,
        "phpdoc_order": true,
        "phpdoc_trim": true,
        "phpdoc_align": true,
        "phpdoc_types": true,
        "phpdoc_single_line_var_spacing": true
    }
}
